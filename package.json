{
    "name": "baobab-client",
    "version": "1.0.0",
    "scripts": {
        "config": "ts-node config-env.ts",
        "ng": "ng",
        "start": "node server.js",
        "start:dev": "npm run config -- && ng serve --aot --proxy-config proxy.conf.json",
        "build": "npm run config -- && ng build --aot",
        "build:heroku": "npm run config -- && ng build --aot --configuration=staging --prod",
        "test": "ng test --watch=false --progress=false --browsers=ChromeHeadless --code-coverage --source-map=false",
        "build:ci": "npm run config -- && ng build --aot --prod --sourceMap=false --base-href baobab-client",
        "test:ci": "npm run config -- && ng test --watch=false --code-coverage",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postbuild": "npm run build:heroku",
        "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/fr.json --clean --format-indentation ' ' --sort --format namespaced-json"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "^0.803.21",
        "@angular/animations": "8.0.0",
        "@angular/cli": "^8.3.21",
        "@angular/common": "^8.2.14",
        "@angular/compiler": "~8.2.13",
        "@angular/compiler-cli": "~8.2.13",
        "@angular/core": "^8.2.14",
        "@angular/forms": "~8.2.13",
        "@angular/platform-browser": "~8.2.13",
        "@angular/platform-browser-dynamic": "~8.2.13",
        "@angular/router": "~8.2.13",
        "@fortawesome/angular-fontawesome": "^0.5.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.14.0",
        "@ng-bootstrap/ng-bootstrap": "^5.3.1",
        "@ngrx/effects": "^8.6.0",
        "@ngrx/store": "^9.1.0",
        "@ngrx/store-devtools": "^9.0.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@rxweb/reactive-form-validators": "^1.9.9-beta2",
        "admin-lte": "^3.0.1",
        "ag-grid-angular": "^23.2.1",
        "ag-grid-community": "^23.2.1",
        "bootstrap": "^4.4.1",
        "clear": "^0.1.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
