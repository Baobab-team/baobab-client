        "jquery": "^3.4.1",
        "jszip": "^3.3.0",
        "messageformat": "^2.0.2",
        "ngx-toastr": "11.3.3",
        "ngx-translate-messageformat-compiler": "^4.4.0",
        "path": "^0.12.7",
        "popper.js": "^1.16.0",
        "rxjs": "~6.4.0",
        "save-dev": "0.0.1-security",
        "serialize-javascript": ">=2.1.1",
